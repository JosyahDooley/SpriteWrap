{
	"is-c3-addon": true,
	"type": "effect",
	"name": "SpriteWrap",
	"id": "SpriteWrap",
	"version": "1.0.0.0",
	"author": "Josyah Dooley",
	"website": "https://www.construct.net",
	"documentation": "https://www.construct.net",
	"description": "Wrap a sprites uv coordinates",
	"file-list": [
		"lang/en-US.json",
		"addon.json",
		"effect.fx"
	],

	"category": "blend",
	"blends-background": true,
	"cross-sampling": false,
	"preserves-opaqueness": false,
	"animated": false,
	"extend-box": {
		"horizontal": 0,
		"vertical": 0
	},
	"is-deprecated": false,

	"parameters": [
		{
			"id": "offsetx",
			"type": "float",
			"initial-value": 0.0,
			"uniform": "offsetX"
		},
		{
			"id": "offsety",
			"type": "float",
			"initial-value": 0.0,
			"uniform": "offsetY"
		}
	]
}
